<!DOCTYPE html>
<html>
  <head>
    <title>E-shop-next</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'home', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="wrapper">
      <div class="d-flex">



        <div class="main">




          <main class="content">
                    <%= render partial: "layouts/home/navbar" %>
          <div class="container py-5">
          <%= render "layouts/shared/flash_messages" %>
              <%= yield %>
            </div>
          </main>

        </div>

      </div>
    </div>
  </body>
</html>
